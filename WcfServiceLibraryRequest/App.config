<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <appSettings>
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
  </appSettings>
  <system.web>
    <compilation debug="true" />
  </system.web>
  <!--При развертывании проекта библиотеки службы необходимо добавить содержимое файла конфигурации в 
  файл app.config узла. Пространство имен System.Configuration не поддерживает файлы конфигурации для библиотек. -->
  <system.serviceModel>
    <services>
      
             
      <service name="WcfServiceLibraryRequest.CRUD">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/WcfServiceLibraryRequest/CRUD/" />
            <add baseAddress="net.tcp://localhost:8733/WcfServiceLibraryRequest/CRUD/" />
          </baseAddresses>
        </host>
        <!-- Service Endpoints -->
        <!-- Если адрес не задан полностью, он является относительным к вышеуказанному базовому адресу -->
        <endpoint address="" binding="basicHttpBinding" contract="WcfServiceLibraryRequest.IBrigadeCRUD"/>
        <endpoint address="" binding="basicHttpBinding" contract="WcfServiceLibraryRequest.IManagerTableCRUD"/>
        <endpoint address="" binding="basicHttpBinding" contract="WcfServiceLibraryRequest.IRequestStateTableCRUD"/>
        <endpoint address="" binding="basicHttpBinding" contract="WcfServiceLibraryRequest.IRequestTableCRUD"/>
        <endpoint address="" binding="basicHttpBinding" contract="WcfServiceLibraryRequest.IRequestTypeTableCRUD"/>

        <endpoint address="" binding="basicHttpBinding" contract="WcfServiceLibraryRequest.ISystemAdministratorTableCRUD"/>
        <endpoint address="" binding="basicHttpBinding" contract="WcfServiceLibraryRequest.IWorkerTableCRUD"/>
        <endpoint address="" binding="basicHttpBinding" contract="WcfServiceLibraryRequest.ISelect"/>
        <endpoint address="" binding="basicHttpBinding" contract="WcfServiceLibraryRequest.IStreetTableCRUD"/>
        
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
      </service>

    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <!--Чтобы избежать раскрытия метаданных, 
          до развертывания задайте следующим параметрам значение "false".-->
          <serviceMetadata httpGetEnabled="True" httpsGetEnabled="True" />
          <!-- Чтобы получить сведения об исключениях в ошибках для отладки, 
          установите ниже значение TRUE. Перед развертыванием установите значение FALSE, 
           чтобы избежать разглашения сведений об исключении -->
          <serviceDebug includeExceptionDetailInFaults="false" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>

